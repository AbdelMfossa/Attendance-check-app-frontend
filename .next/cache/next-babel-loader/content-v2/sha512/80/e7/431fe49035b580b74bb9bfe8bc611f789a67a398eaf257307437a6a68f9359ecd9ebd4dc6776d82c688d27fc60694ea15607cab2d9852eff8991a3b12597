ow={this.state.show}\n          onHide={this.handleClose}\n          backdrop=\"static\"\n          keyboard={false}\n          className=\"modalSuppression\"\n        >\n          <Modal.Header closeButton className=\"color-titre-ajout\">\n            <Modal.Title className=\"colorTitre\">{`Ajout d'un ${this.props.title}`}</Modal.Title>\n          </Modal.Header>\n          <Modal.Body>\n            <div className=\"modal-form\">\n              <form>\n                <div>\n                  <label>Noms</label>\n                  <input\n                    type=\"text\"\n                    className=\"form-control\"\n                    placeholder=\"\"\n                    onChange={(e) => (this.state.first_name = e.target.value)}\n                  />\n                </div>\n                <div>\n                  <label>Prénom</label>\n                  <input\n                    type=\"text\"\n                    className=\"form-control\"\n                    placeholder=\"\"\n                    onChange={(e) => (this.state.last_name = e.target.value)}\n                  />\n                </div>\n                <div>\n                  <label>Matricule</label>\n                  <input\n                    type=\"text\"\n                    className=\"form-control\"\n                    placeholder=\"\"\n                    onChange={(e) => (this.state.matricule = e.target.value)}\n                  />\n                </div>\n                <div>\n                  <label>Numéro de téléphone</label>\n                  <input\n                    type=\"tel\"\n                    maxLength=\"9\"\n                    minLength=\"9\"\n                    className=\"form-control\"\n                    placeholder=\"\"\n                    onChange={(e) => (this.state.phone = e.target.value)}\n                  />\n                </div>\n                <div>\n                  <label>Genre</label>\n                  <select\n                    className=\"form-control\"\n                    onChange={(e) => this.setState({ genre: e.target.value })}\n                  >\n                    <option value=\"masculin\">masculin</option>\n                    <option value=\"feminin\">féminin</option>\n                  </select>\n                </div>\n                <div>\n                  <label>Grade</label>\n                  <select\n                    className=\"form-control\"\n                    onChange={(e) => {\n                      e.target.value == \"1\"\n                        ? this.setState({ grade: true })\n                        : this.setState({ grade: grade });\n                    }}\n                  >\n                    <option value=\"0\">Surveillant</option>\n                    <option value=\"1\">Chef de salle</option>\n                  </select>\n                </div>\n              </form>\n            </div>\n          </Modal.Body>\n          <Modal.Footer>\n            <Button variant=\"secondary\" onClick={this.handleClose}>\n              Fermer\n            </Button>\n            <Button\n              variant=\"primary\"\n              type=\"submit\"\n              onClick={this.handleCreate}\n              className=\"color-titre-ajout\"\n            >\n              Valider\n            </Button>\n          </Modal.Footer>\n        </Modal>\n      </>\n    );\n  }\n}\n\nexport default CustomModal;\n"]},"metadata":{},"sourceType":"module"}